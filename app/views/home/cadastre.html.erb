        <%= render :partial => 'header', :locals => {:page => 'loja'} %>
        
        <div class="stores">
        	<div>
        		<div class="content center">
        			<ul class="states">
        				<% @states.each do |state| %>
        					<li>
                                <%
                                
                                case state
                                when 'SP'
                                    concat 'São Paulo'
                                when 'RJ'
                                    concat 'Rio de Janeiro'
                                when 'SC'
                                    concat 'Santa Catarina'
                                when 'RS'
                                    concat 'Rio Grande do Sul'
                                when 'PR'
                                    concat 'Paraná'
                                when 'AP'
                                    concat 'Amapá'
                                else
                                    concat ''
                                end
                                
                                %>
                            </li>
        				<% end %>
        			</ul>
        		</div>
        	</div>
        </div>
        
        <div class="row content text-center">
            <div class="col-md-12">
                <h2 class="fundo-faixa">Cadastre-se</h2>
                <%= form_for :user, url: novouser_path do |f| %>
                    <div class="row formcep">
                        <div class="col-md-6">
                            <%= f.text_field :name, :placeholder => 'Nome', :class => 'form-control end' %>
                        </div>
                        <div class="col-md-6">
                            <%= f.text_field :cpf,  :placeholder => 'CPF', :class => 'form-control end' %>
                        </div>
                    </div>
                    <div class="row formcep">
                        <div class="col-md-6">
                            <%= f.text_field :email, :placeholder => 'Email', :class => 'form-control end' %>
                        </div>
                        <div class="col-md-6">
                            <%= f.text_field :password, :placeholder => 'Senha', :class => 'form-control end' %>
                        </div>
                    </div>
                    <%= f.submit 'Cadastrar', :class => 'btn-cep' %>

                <% end %>
            </div>
        </div><br>
        <br>
        <%= render 'footer' %>

        <script type="text/javascript" src="/js/jquery-3.1.1.min.js"></script>
        <script type="text/javascript" src="/js/bootstrap.js"></script>
        <script type="text/javascript" src="/js/main.js"></script>
        <script type="text/javascript" src="/js/mask.js"></script>
        <script type="text/javascript">
            $(function(){

                $('#user_cpf').mask('999.999.999-99');

            });
        </script>

        <script type="text/javascript" async src="https://d335luupugsy2.cloudfront.net/js/loader-scripts/06c0c772-6f76-429f-8082-c86ab41812f5-loader.js" ></script>
